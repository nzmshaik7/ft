<h1>Listing ts_band_recalls</h1>

<table>
  <tr>
    <th>Vehicle</th>
    <th>Name</th>
    <th>Date completed</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ts_band_recalls.each do |ts_band_recall| %>
  <tr>
    <td><%= ts_band_recall.vehicle_id %></td>
    <td><%= ts_band_recall.name %></td>
    <td><%= ts_band_recall.date_completed %></td>
    <td><%= link_to 'Show', ts_band_recall %></td>
    <td><%= link_to 'Edit', edit_ts_band_recall_path(ts_band_recall) %></td>
    <td><%= link_to 'Destroy', ts_band_recall, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

