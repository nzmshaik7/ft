<h1>Listing part_manufacturers</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @part_manufacturers.each do |part_manufacturer| %>
  <tr>
    <td><%= part_manufacturer.name %></td>
    <td><%= link_to 'Show', part_manufacturer %></td>
