<%= form_for(@code_history) do |f| %>
  <% if @code_history.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@code_history.errors.count, "error") %> prohibited this code_history from being saved:</h2>

      <ul>
      <% @code_history.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :vehicle_id, 'Vehicle' %><br />
    <%= select("code_history", "vehicle_id", @vehicleCollect,
                                                {prompt: 'Select Vehicle'}) %>
  </div>
  <div class="field">
    <%= f.label :cdate %><br />
    <%= f.date_select :cdate %>
  </div>
  <div class="field">
    <%= f.label :code %><br />
    <%= f.text_field :code %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
