<h1>Listing parts</h1>

<table>
  <tr>
    <th>Part manufacturer</th>
    <th>Part name</th>
    <th>Description</th>
    <th>Part number</th>
    <th>Actual cost</th>
    <th>Retail price</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @parts.each do |part| %>
  <tr>
    <td><%= part.part_manufacturer_id %></td>
    <td><%= part.part_name_id %></td>
    <td><%= part.description %></td>
    <td><%= part.part_number %></td>
    <td><%= part.actual_cost %></td>
    <td><%= part.retail_price %></td>
    <td><%= link_to 'Show', part %></td>
    <td><%= link_to 'Edit', edit_part_path(part) %></td>
    <td><%= link_to 'Destroy', part, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Part', new_part_path %>
