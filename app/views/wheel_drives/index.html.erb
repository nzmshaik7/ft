<h2>Listing wheel_drives</h2>

<table border="1" class="ft_dbtab">
  <tr bgcolor="#F0FFF0">
    <th>Name</th>
    <th colspan="3"><i>Actions</i></th>
  </tr>

<% @wheel_drives.each do |wheel_drife| %>
  <tr>
    <td><%= wheel_drife.name %></td>
    <td><%= link_to 'Show', wheel_drife %></td>
    <td><%= link_to 'Edit', edit_wheel_drife_path(wheel_drife) %></td>
    <td><%= link_to 'Destroy', wheel_drife, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Wheel drive', new_wheel_drife_path %>
