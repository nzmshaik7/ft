<h2>Listing service_line_items</h2>

<table border="1" class="ft_dbtab">
  <tr bgcolor="#F0FFF0">
    <th>Service visit</th>
    <th>Service description</th>
    <th>Service description text</th>
    <th>Type</th>
    <th colspan="3"><i>Actions</i></th>
  </tr>

<% @service_line_items.each do |service_line_item| %>
  <tr>
    <td><%= service_line_item.service_visit.visitText %></td>
    <td><%= service_line_item.service_description.name %></td>
    <td><%= service_line_item.service_description_text %></td>
    <td><%= stypeText(service_line_item.stype) %></td>
    <td><%= link_to 'Show', service_line_item %></td>
    <td><%= link_to 'Edit', edit_service_line_item_path(service_line_item) %></td>
    <td><%= link_to 'Destroy', service_line_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Service line item', new_service_line_item_path %>
