<!DOCTYPE html>
<html>
<head>
  <title>Futuretech</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <% if params[:controller] == 'mockup' or params[:controller] == 'analytics' %>
    <%= stylesheet_link_tag 'mockup/mcommon' %>
    <%= stylesheet_link_tag 'mockup/mock' %>
    <%= stylesheet_link_tag 'mockup/bootstrap-popbase' %>
    <%= stylesheet_link_tag 'mockup/unoverride' %>
  <% end %>
  <%= javascript_include_tag "application" %>
  <% if params[:controller] == 'mockup' or params[:controller] == 'analytics' %>
    <%= javascript_include_tag 'mockup/bootstrap-tooltip' %>
    <%= javascript_include_tag 'mockup/bootstrap-popover' %>
  <% end %>
  <% if params[:controller] == 'mockup' %>
    <%= javascript_include_tag 'mockup/mockup' %>
  <% end %>
  <% if params[:controller] == 'analytics' %>
    <%= javascript_include_tag 'intPopover' %>
  <% end %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render :partial => 'layouts/headnav' %>
<%= render :partial => 'layouts/flash' %>

<%= yield %>

</body>
</html>
