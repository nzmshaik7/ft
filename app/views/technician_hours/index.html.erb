<h1>Listing technician_hours</h1>

<table border="1" class="ft_dbtab">
  <tr>
    <th>Service line item</th>
    <th>Labor hours retail</th>
    <th>Labor rate retail</th>
    <th>Labor hours actual</th>
    <th>Labor rate actual</th>
    <th>Technician</th>
    <th colspan="3"><i>Actions</i></th>
  </tr>

<% @technician_hours.each do |technician_hour| %>
  <tr>
    <td>
      <a href="/service_line_items/<%= technician_hour.service_line_item_id %>">
      <%= technician_hour.service_line_item_id %>
      </a>
    </td>
    <td><%= technician_hour.labor_hours_retail %></td>
    <td><%= technician_hour.labor_rate_retail %></td>
    <td><%= technician_hour.labor_hours_actual %></td>
    <td><%= technician_hour.labor_rate_actual %></td>
    <td>
      <%= technician_hour.technician_id %>/
      <%= technician_hour.technician.employee.nameText %>
    </td>
    <td><%= link_to 'Show', technician_hour %></td>
    <td><%= link_to 'Edit', edit_technician_hour_path(technician_hour) %></td>
    <td><%= link_to 'Destroy', technician_hour, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Technician hour', new_technician_hour_path %>
