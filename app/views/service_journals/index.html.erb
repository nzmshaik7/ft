<h1>Listing service_journals</h1>

<table>
  <tr>
    <th>Service visit</th>
    <th>Event</th>
    <th>Date time</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @service_journals.each do |service_journal| %>
  <tr>
    <td><%= service_journal.service_visit_id %></td>
    <td><%= service_journal.event_id %></td>
    <td><%= service_journal.date_time %></td>
    <td><%= link_to 'Show', service_journal %></td>
    <td><%= link_to 'Edit', edit_service_journal_path(service_journal) %></td>
    <td><%= link_to 'Destroy', service_journal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

