<h1>Listing manufacturer_warranties</h1>

<table>
  <tr>
    <th>Vehicle</th>
    <th>Months</th>
    <th>Miles</th>
    <th>Wtype</th>
    <th>Image</th>
    <th>Comment</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @manufacturer_warranties.each do |manufacturer_warranty| %>
  <tr>
    <td><%= manufacturer_warranty.vehicle_id %></td>
    <td><%= manufacturer_warranty.months %></td>
    <td><%= manufacturer_warranty.miles %></td>
    <td><%= manufacturer_warranty.wtype %></td>
    <td><%= manufacturer_warranty.image %></td>
    <td><%= manufacturer_warranty.comment %></td>
    <td><%= link_to 'Show', manufacturer_warranty %></td>
    <td><%= link_to 'Edit', edit_manufacturer_warranty_path(manufacturer_warranty) %></td>
    <td><%= link_to 'Destroy', manufacturer_warranty, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

