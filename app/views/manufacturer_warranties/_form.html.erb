<%= form_for(@manufacturer_warranty) do |f| %>
  <% if @manufacturer_warranty.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@manufacturer_warranty.errors.count, "error") %> prohibited this manufacturer_warranty from being saved:</h2>

      <ul>
      <% @manufacturer_warranty.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :vehicle_id %><br />
    <%= select("manufacturer_warranty", "vehicle_id", @vehicleCollect,
                                          {prompt: 'Select Vehicle'}) %>
  </div>
  <div class="field">
    <%= f.label :months %><br />
    <%= f.number_field :months %>
  </div>
  <div class="field">
    <%= f.label :miles %><br />
    <%= f.number_field :miles %>
  </div>
  <div class="field">
    <%= f.label :manufacturer_warranty_type_id, 'Warranty Type' %><br />
    <%= select("manufacturer_warranty", "manufacturer_warranty_type_id", 
                    @warrantyTypeCollect, {prompt: 'Select Warranty Type'}) %>
  </div>
  <div class="field">
    <%= f.label :image %><br />
    <%= f.number_field :image %>
  </div>
  <div class="field">
    <%= f.label :comment %><br />
    <%= f.text_field :comment %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
