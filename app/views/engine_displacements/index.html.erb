<h1>Listing engine_displacements</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @engine_displacements.each do |engine_displacement| %>
  <tr>
    <td><%= engine_displacement.name %></td>
    <td><%= link_to 'Show', engine_displacement %></td>
    <td><%= link_to 'Edit', edit_engine_displacement_path(engine_displacement) %></td>
    <td><%= link_to 'Destroy', engine_displacement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

